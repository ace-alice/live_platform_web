{
  "name": "live-platform-web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode fat --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:fat": "vue-tsc --noEmit && vite build --mode fat",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod",
    "build:all": "vue-tsc --noEmit && vite build --mode fat && vite build --mode prod",
    "serve": "http-server ./dist/hyh-fat -o",
    "serve:prod": "http-server ./dist/hyh-prod -o --gzip",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "format": "prettier --write src/",
    "new": "plop",
    "svgo": "svgo -f src/assets/icons",
    "preinstall": "npx only-allow pnpm",
    "lint:tsc": "vue-tsc --noEmit",
    "lint:eslint": "eslint src/**/*.{ts,tsx,vue} --cache --fix",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "postinstall": "simple-git-hooks && esno scripts/prepare.js"
  },
  "dependencies": {
    "dayjs": "^1.11.19",
    "default-passive-events": "^4.0.0",
    "echarts": "^6.0.0",
    "element-plus": "^2.13.1",
    "mitt": "^3.0.1",
    "pinia": "^3.0.4",
    "swiper": "^12.0.3",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4",
    "xgplayer": "^3.0.23",
    "xgplayer-flv.js": "^3.0.23",
    "xgplayer-hls.js": "^3.0.23"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.2.3",
    "@iconify/vue": "^5.0.0",
    "@rushstack/eslint-patch": "^1.15.0",
    "@tsconfig/node20": "^20.1.8",
    "@types/node": "^25.0.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all2": "^8.0.4",
    "plop": "^4.0.4",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^11.1.1",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.8.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "sass": "^1.97.2",
    "simple-git-hooks": "^2.13.1",
    "svgo": "^4.0.0",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "unplugin-vue-router": "^0.19.2",
    "vite": "^7.3.1",
    "vite-plugin-banner": "^0.8.1",
    "vite-plugin-cdn2": "^1.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-plugin-pages": "^0.33.2",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^3.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "preserveUnused": true
  },
  "engines": {
    "node": ">=22"
  }
}
